	<view class="h6" style="#595959">资料状态</view>
	<view class="flex-1"></view>
	<view class="h6" style="color:#4867AA">未完善</view>
</view> -->

<view style="height:150rpx"></view>
<view wx:if="{{orderinfo.orderstatus=='A'}}" class="bom bg-white ">
	<view class="flex-row flex-center padding-20">

		<view class="h6" style="color:#000000">定金:</view>
		<view class="h6 margin-left-20" style="color:#FF0027">￥</view>
		<view class="f-36" style="color:#FF3333">{{orderinfo.amount}}</view>
		<view class="flex-1"></view>

		<view class="queren1" bindtap="quxiaodindan">取消订单</view>
		<view class="queren margin-left-10" bindtap="zhifudinjin">支付定金</view>

	</view>

</view>