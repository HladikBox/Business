			<view bindtap="xzguojia" wx:for="{{swiperList}}" wx:key='k' data-id="{{item.id}}" class=" {{xzguojia==item.id?'xzguojia':'guojia'}}">{{item.name}}</view>
		</scroll-view>
		<scroll-view scroll-y class="flex-1">
			<view wx:if="{{xzguojia!=undefined}}" bindtap="xzchenshi" data-id='0' class=" {{xzchenshi==0?'xzchenshi':'chengshi'}}">全国</view>
			<view bindtap="xzchenshi" wx:for="{{chenshilist}}" wx:key='k' data-id="{{item.id}}"
