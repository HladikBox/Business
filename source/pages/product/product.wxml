			<view bindtap="fanxinquedin" data-id='1' class=" flex-1 jgqd">确定</view>
		</view>
	</view>
	<!-- 房产排序 -->
	<view wx:if="{{fcpx==true}}" class="fcpx">
		<view bindtap="fanchanpaixu" data-id="0" class="margin-left-40 margin-top-20 margin-bottom-10 f-22" style="color:#595959">默认</view>
		<view bindtap="fanchanpaixu" data-id="1" class="margin-left-40 margin-top-20 margin-bottom-10 f-22" style="color:#595959">最新</view>
		<view bindtap="fanchanpaixu" data-id="2" class="margin-left-40 margin-top-20 margin-bottom-10 f-22" style="color:#595959">总价从低到高</view>
		<view bindtap="fanchanpaixu" data-id="3" class="margin-left-40 margin-top-20 margin-bottom-10 f-22" style="color:#595959">总价从高到低</view>
		<view bindtap="fanchanpaixu" data-id="4" class="margin-left-40 margin-top-20 margin-bottom-10 f-22" style="color:#595959">单价从高到低</view>
		<view bindtap="fanchanpaixu" data-id="5" class="margin-left-40 margin-top-20 margin-bottom-10  f-22" style="color:#595959">单价从低到高</view>
	</view>
	<view catchtouchmove='true' bindtap="guanbi" wx:if="{{shaixuan==true}}" class="balckbg"></view>
	<view wx:for="{{houselist}}" data-id="{{item.id}}" wx:key='k' bindtap="productinfo" class="padding-40 divx  flex-row flex-center">
		<view>
			<image class="icon-100" src="{{uploadpath}}houseproject/{{item.cover}}"></image>
			<view class="chenshi">{{item.country_name}}·{{item.city_name}}</view>
		</view>
		<view class="flex-1">
			<view class=" flex-1 flex-row flex-center margin-left-20">
				<view>
					<view class="h5" style="color:#161616">
						{{item.name}}
					</view>
					<view class="h7" style="color:#595959">
						{{item.housewear}} | {{item.areacovered}}
					</view>
				</view>
				<view class="flex-1"></view>
				<view class="txt-bold" style="color:#FF4466">

					￥{{item.pricesection_num}}







					<text class="h7" style="color:#FF4466">万起</text>
				</view>
			</view>
			<view class="flex-row flex-1 flex-center margin-top-15  margin-left-20">
				<view class="tag">近一年房价均涨{{item.priceincrease}}%</view>
				<view class="flex-1"></view>
				<view class="tag">年均租金{{item.annualrent}}%</view>
				<view class="flex-1"></view>
				<view class="tag">首付比例{{item.downpayments}}%</view>
			</view>
			<view class="flex-row flex-1 flex-center margin-top-15  margin-left-20">
				<view class="h7" style="color:#777777">{{item.fanxintext}}</view>
				<view class="h7 margin-left-10" style="color:#777777">{{item.duration}}</view>
			</view>
			<view class="flex-row flex-1 flex-center margin-top-15  margin-left-20">
				<view class="tag2">佣金比例{{item.commissionrate}}%</view>
				<view class="flex-1"></view>
				<view class="tag2">佣金收入{{item.commissionincome}}万</view>
				<view class="flex-1"></view>
				<view class="tag2">结佣时间{{item.settlementdate}}天</view>
			</view>
		</view>
	</view>
</view>
